--echo Testing multi-line SQL parsing

# Test 1: Simple multi-line CREATE TABLE
CREATE TABLE test_table (
    id INT PRIMARY KEY,
    name VARCHAR(50)
);

# Test 2: Multi-line INSERT
INSERT INTO test_table 
    (id, name) 
VALUES 
    (1, 'John'),
    (2, 'Jane');

# Test 3: Multi-line SELECT
SELECT 
    id,
    name
FROM test_table
WHERE id > 0;

# Clean up
DROP TABLE test_table;