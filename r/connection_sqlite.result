SELECT 'default_sqlite_connection' as connection_type
default_sqlite_connection
创建新的 SQLite 文件连接
CREATE TABLE file_table (id INTEGER PRIMARY KEY, data TEXT)
INSERT INTO file_table VALUES (1, 'data_from_file_conn')
