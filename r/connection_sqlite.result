SELECT * FROM file_table
1	data_from_file_conn
SELECT * FROM file_table2
1	data_from_file_conn2
切换到第二个文件连接
SELECT * FROM file_table2
1	data_from_file_conn2
切换回默认内存连接
CREATE TABLE memory_table (id INTEGER, value TEXT)
INSERT INTO memory_table VALUES (1, 'memory_data')
SELECT * FROM file_table
1	data_from_file_conn
